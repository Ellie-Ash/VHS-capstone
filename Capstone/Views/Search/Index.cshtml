@model Capstone.Models.ViewModels.AllUsersViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form class="form-inline my-2 my-lg-0" asp-area="" asp-controller="Search" asp-action="Index">
    <input class="form-control mr-sm-2" name="searchString" type="search" placeholder="" aria-label="Search">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
</form>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.AllUsers) {
        <tr>
            <p>@item.UserName</p>
        </tr>
}
    </tbody>
</table>
